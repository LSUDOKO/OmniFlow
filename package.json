{
  "name": "omniflow-rwa-marketplace",
  "version": "1.0.0",
  "description": "Production-ready cross-chain RWA marketplace with OneChain and SUI compatibility",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "hardhat compile --config hardhat.config.js",
    "test": "hardhat test",
    "deploy:ethereum": "hardhat run scripts/deploy.js --network ethereum",
    "deploy:polygon": "hardhat run scripts/deploy.js --network polygon",
    "deploy:bsc": "hardhat run scripts/deploy.js --network bsc",
    "deploy:onechain": "hardhat run scripts/deploy.js --network onechain_testnet",
    "deploy:onechain-mainnet": "hardhat run scripts/deploy.js --network onechain_mainnet",
    "deploy:all": "npm run deploy:ethereum && npm run deploy:polygon && npm run deploy:bsc && npm run deploy:onechain",
    "verify:contracts": "hardhat run scripts/verify.js",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "^3.0.0",
    "@thirdweb-dev/react": "^4.0.0",
    "@thirdweb-dev/sdk": "^4.0.0",
    "thirdweb": "^5.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/next-js": "^2.2.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "wagmi": "^2.0.0",
    "viem": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "ethers": "^6.8.0",
    "axios": "^1.6.0",
    "react-icons": "^4.12.0",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.0",
    "yup": "^1.3.0",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0",
    "react-dropzone": "^14.2.0",
    "ipfs-http-client": "^60.0.0",
    "@mysten/sui.js": "^0.54.0",
    "@mysten/wallet-adapter-react": "^0.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "hardhat": "^2.19.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "keywords": [
    "RWA",
    "Real World Assets",
    "Cross-chain",
    "OneChain",
    "SUI",
    "NFT",
    "DeFi",
    "Tokenization",
    "Marketplace"
  ],
  "author": "OmniFlow Team",
  "license": "MIT"
}
